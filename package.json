{
  "name": "mobix-web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22",
    "yarn": ">=1.22"
  },
  "scripts": {
    "dev:next": "next dev -p 4567 --turbopack",
    "dev:ngrok": "ngrok start mobix_web --config=ngrok.yml",
    "dev": "concurrently -k -n next,ngrok -c blue,magenta \"yarn dev:next\" \"yarn dev:ngrok\"",
    "dev:test": "NEXT_PUBLIC_ENVIRONMENT=test NEXT_PUBLIC_USE_NGROK=false next dev -p 4567 --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test -c playwright.config.ts",
    "test:e2e:ui": "playwright test -c playwright.config.ts --ui",
    "test:e2e:headed": "playwright test -c playwright.config.ts --headed",
    "test:ct": "playwright test -c playwright-ct.config.ts",
    "test:ct:ui": "playwright test -c playwright-ct.config.ts --ui",
    "test:ct:headed": "playwright test -c playwright-ct.config.ts --headed",
    "test:storybook": "vitest --config vitest.storybook.config.ts run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.9",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/x-data-grid": "^8.19.0",
    "@mui/x-date-pickers": "^8.19.0",
    "@reduxjs/toolkit": "^2.10.1",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "next": "15.3.2",
    "openai": "^4.80.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-number-format": "^5.4.4",
    "react-phone-number-input": "^3.4.14",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/eslintrc": "^3",
    "@playwright/experimental-ct-react": "1.57.0",
    "@playwright/test": "1.57.0",
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-onboarding": "^10.0.8",
    "@storybook/addon-vitest": "^10.0.8",
    "@storybook/nextjs-vite": "^10.0.8",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.0.8",
    "jsdom": "^26.1.0",
    "ngrok": "^5.0.0-beta.2",
    "playwright": "1.57.0",
    "storybook": "^10.0.8",
    "typescript": "^5",
    "vite": "^5.4.9",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "react-hook-form": "^7.67.0"
  }
}
